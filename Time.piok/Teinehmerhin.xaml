<Window x:Class="Time.piok.Teinehmerhin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Teinehmer hinzufügen" Height="300" Width="512.538">
    
    <Grid Margin="0,0,4,0.4">
        <Label Content="Vorname" HorizontalAlignment="Left" Height="28" Margin="27,38,0,0" VerticalAlignment="Top" Width="80"/>
        <Label Content="Startnummer" HorizontalAlignment="Left" Height="28" Margin="27,104,0,0" VerticalAlignment="Top" Width="80"/>
        <Label Content="Nachname" HorizontalAlignment="Left" Height="28" Margin="27,71,0,0" VerticalAlignment="Top" Width="80"/>
        <ComboBox Name="cb_cat" HorizontalAlignment="Left" Height="25" Margin="138,161,0,0" VerticalAlignment="Top" Width="325"/>
        <Label Content="Klasse" HorizontalAlignment="Left" Margin="27,161,0,0" VerticalAlignment="Top"/>
        <TextBox Name="name_txt" HorizontalAlignment="Left" Height="23" Margin="138,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="113" RenderTransformOrigin="0.56,-0.155"/>
        <TextBox Name="stnr_txt" HorizontalAlignment="Left" Height="23" Margin="138,107,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="64" RenderTransformOrigin="0.56,-0.155"/>
        <TextBox Name="vname_txt" HorizontalAlignment="Left" Height="23" Margin="138,43,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="113" RenderTransformOrigin="0.56,-0.155"/>
        <Button  Name="btn_add" Content="Hinzufügen" HorizontalAlignment="Left" Height="59" Margin="27,201,0,0" VerticalAlignment="Top" Width="436" Click="btn_add_Click"/>
        <Label Content="Geschlecht" HorizontalAlignment="Left" Height="24" Margin="27,137,0,0" VerticalAlignment="Top" Width="70"/>
        <ComboBox Name="cb_ges" HorizontalAlignment="Left" Height="21" Margin="138,135,0,0" VerticalAlignment="Top" Width="196">
            <ComboBoxItem Content="Männlich"/>
            <ComboBoxItem Content="Weiblich"/>
                   </ComboBox>
    </Grid>
</Window>
