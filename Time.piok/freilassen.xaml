<Window x:Class="Time.piok.freilassen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="freilassen" Height="200" Width="300">
    <Grid>
        <Button Content="OK" HorizontalAlignment="Left" Height="51" Margin="0,119,-0.4,0" VerticalAlignment="Top" Width="294" Click="Button_Click"/>
        <CheckBox Name="cb_frei" Content="Zwischen Kategorien freilassen?" HorizontalAlignment="Left" Height="22" Margin="10,34,0,0" VerticalAlignment="Top" Width="188" Checked="cb_frei_Checked" Unchecked="cb_frei_Unchecked"/>
        <Label Content="Zwischen den Kategorien" HorizontalAlignment="Left" Height="26" Margin="10,74,0,0" VerticalAlignment="Top" Width="144"/>
        <TextBox Name="txt_anz" IsEnabled="False" HorizontalAlignment="Left" Height="26" Margin="154,74,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <Label Content="Nummern" HorizontalAlignment="Left" Height="26" Margin="183,74,0,0" VerticalAlignment="Top" Width="86"/>

    </Grid>
</Window>
