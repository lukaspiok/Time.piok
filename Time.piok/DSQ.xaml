<Window x:Class="Time.piok.DSQ"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DSQ" Height="200" Width="200">
    <Grid>
        <Label Content="Startnummer" HorizontalAlignment="Left" Height="26" Margin="10,33,0,0" VerticalAlignment="Top" Width="80"/>
        <Label Content="Tornummer" HorizontalAlignment="Left" Height="26" Margin="10,75,0,0" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="txt_stnr" HorizontalAlignment="Left" Height="26" Margin="109,33,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="64"/>
        <TextBox x:Name="txt_tornr" HorizontalAlignment="Left" Height="26" Margin="109,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="64"/>
        <Button Content="OK" x:Name="btn_dsq" HorizontalAlignment="Left" Height="39" Margin="10,121,0,0" VerticalAlignment="Top" Width="163" Click="btn_dsq_Click"/>

    </Grid>
</Window>
